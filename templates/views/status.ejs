    <body>

        <div class="noderow">
            <span class="node">
                <strong>Node 1 Stats</strong>
                <% if(nodeData.get(`node1`)) { %>
                    <div class="nodeinfo">
                        <strong style="color: green;">Online</strong>
                        <br>
                        <br>
                        <strong>CPU</strong>
                        <br>
                        Usage >> <%- nodeData.fetch("node1").cpu %>%
                            <br>
                            Load >> <%- nodeData.fetch("node1").cpuload %>
                                <br>
                                <strong>RAM</strong>
                                <br>
                                Usage >> <%- prettybytes(nodeData.fetch("node1").memused * 1000) %>
                                    <br>
                                    Total >> <%- prettybytes(nodeData.fetch("node1").memtotal * 1000) %>
                                        <br>
                                        <strong>Disk</strong>
                                        <br>
                                        Usage >> <%- prettybytes(nodeData.fetch("node1").diskused * 1000000) %>
                                            <br>
                                            Total >> <%- prettybytes(nodeData.fetch("node1").disktotal * 1000000) %>
                                                <br>
                                                <strong>Last Checked</strong>
                                                <br>
                                                Date >> Now lol
                    </div>
                    <% } else { %>
                        <div class="nodeinfo">
                            <strong style="color: RED;">Offline</strong>
